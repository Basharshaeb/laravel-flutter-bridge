{
    "name": "laravel-flutter/generator",
    "description": "A Laravel package that generates Flutter features (models, services, UI components) based on Laravel models and routes",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "flutter",
        "generator",
        "code-generation",
        "api",
        "mobile"
    ],
    "authors": [
        {
            "name": "BasharShaeb",
            "email": "sssbas228@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "LaravelFlutter\\Generator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFlutter\\Generator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFlutter\\Generator\\FlutterGeneratorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "analyse": "vendor/bin/phpstan analyse"
    }
}
